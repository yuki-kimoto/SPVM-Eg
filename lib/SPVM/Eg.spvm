  static method text : Eg::Element ($text : string) {
    
    unless ($text) {
      die "\$text must be defined.";
    }
    
    my $new_element = Eg::Element->new;
    
    $new_element->{type} = Eg::Element->TYPE_TEXT;
    
    my $text_buffer = StringBuffer->new;
    
    $text_buffer->push($text);
    
    $new_element->{text_buffer} = $text_buffer;
    
    return $new_element;
  }
