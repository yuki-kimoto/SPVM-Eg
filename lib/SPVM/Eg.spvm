  static method tag : Eg::Element ($tag_name : string, $elements : Eg::Element[] = undef, $attributes : object[] = undef) {
    
    my $new_element = Eg::Element->new;
    
    $new_element->{type} = Eg::Element->TYPE_ELEMENT;
    
    $new_element->{tag_name} = $tag_name;
    
    $new_element->{elements} = List->new($elements);
    
    $new_element->{attributes} = Hash->new($attributes);
    
    return $new_element;
  }
  
  static method a : Eg::Element ($elements : Eg::Element[] = undef, $attributes : object[] = undef) {
    return &tag("a", $elements, $attributes);
  }


