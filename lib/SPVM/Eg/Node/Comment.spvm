# CharacterData in JavaScript
class Eg::Node::Comment extends Eg::Node::CharacterData {
  allow Eg;
  
  # Class Methods
  static method new : Eg::Node::Comment ($text : string) {
    
    my $self = new Eg::Node::Comment;
    
    $self->SUPER::init(Eg::Node->TYPE_COMMENT_NODE, $text);
    
    return $self;
  }
  
}
