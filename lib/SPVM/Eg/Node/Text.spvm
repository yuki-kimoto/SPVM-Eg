# Text in JavaScript
class Eg::Node::Text extends Eg::Node::CharacterData {
  allow Eg;
  allow Eg::Node::Document;
  
  # Class Methods
  static method new : Eg::Node::Text ($text : string) {
    
    my $self = new Eg::Node::Text;
    
    $self->SUPER::init(Eg::Node->TYPE_TEXT_NODE, $text);
    
    return $self;
  }
  
}
